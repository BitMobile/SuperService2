<?xml version="1.0" encoding="utf-8"?>

<c:Component xmlns:c="BitMobile.ClientModel3.UI" xmlns:s="BitMobile.ValueStack">
  <c:VerticalLayout CssClass="TabBarMainVL">
    <c:HorizontalLine CssClass="TabBarBreaker" />

    <c:HorizontalLayout Id="TabBarLayout" CssClass="TabBarHL">
      <c:VerticalLayout CssClass="TabBarItemVL" Id="TabBarFirstTabButton" OnClick="TabBarFirstTabButton_OnClick">
        <c:VerticalLayout CssClass="TabImageEvents" Id="TabBarFirstTabImage" />
        <c:TextView Text="#orders#" CssClass="TabTextViewNotActive" Id="TabBarFirstTabTextView" />
      </c:VerticalLayout>

      <c:VerticalLayout CssClass="TabBarItemVL" Id="TabBarSecondButton" OnClick="TabBarSecondTabButton_OnClick">
        <c:VerticalLayout CssClass="TabImageClients" Id="TabBarSecondTabImage" />
        <c:TextView Text="#clients#" CssClass="TabTextViewNotActive" Id="TabBarSecondTabTextView" />
      </c:VerticalLayout>

      <c:VerticalLayout CssClass="TabBarItemVL" Id="TabBarThirdTabButton" OnClick="TabBarThirdButton_OnClick">
        <c:VerticalLayout CssClass="TabImageKKT" Id="TabBarThirdTabImage" />
        <c:TextView Text="#registrator#" CssClass="TabTextViewNotActive" Id="TabBarThirdTabTextView" />
      </c:VerticalLayout>

      <c:VerticalLayout CssClass="TabBarItemVL" Id="TabBarFourthButton" OnClick="TabBarFourthButton_OnClick">
        <c:VerticalLayout CssClass="TabImageSettings" Id="TabBarFourthTabImage" />
        <c:TextView Text="#settings#" CssClass="TabTextViewNotActive" Id="TabBarFourthTabTextView" />
      </c:VerticalLayout>

    </c:HorizontalLayout>
  </c:VerticalLayout>
</c:Component>